/*******************************
        Theme Overrides
*******************************/

@menuColor:  #564535;

.ui.menu .active.item,
.ui.menu .active.item:hover,
.ui.menu a.item:hover {
    background-color: @darkOrange;
    color: #fff;
}

.ui.menu .item {
    color: @menuColor;
    font-weight: 500;
    text-transform: none!important;
    font-size: 1.1rem;
    font-weight: bold;
}
// #564535


.ui.form .searchbox.field {
    display: flex;
    align-items: center;
}