{
  "name": "fise-frontend",
  "description": "Frontend server for FISE",
  "license": "MIT",
  "version": "1.0.1",
  "scripts": {
    "start": "razzle start",
    "postinstall": "yarn omelette && yarn patches",
    "omelette": "ln -sf node_modules/@plone/volto/ omelette",
    "patches": "/bin/bash patches/patchit.sh > /dev/null 2>&1 ||true",
    "build": "razzle build",
    "start:prod": "NODE_ENV=production node build/server.js",
    "i18n": "NODE_ENV=production node node_modules/@plone/volto/src/i18n.js",
    "develop:npx": "npx -p mrs-developer missdev --config=jsconfig.json --output=develop",
    "develop": "missdev --config=jsconfig.json --output=develop"
  },
  "workspaces": [
    "src/develop/volto-addons",
    "src/develop/volto-datablocks",
    "src/develop/volto-mosaic",
    "src/develop/volto-addons",
    "src/develop/volto-ckeditor",
    "src/develop/volto-plotlycharts",
    "src/develop/volto-base",
    "src/develop/volto-embed",
    "src/develop/volto-tabsview"
  ],
  "addons": [
    "volto-addons",
    "volto-datablocks",
    "volto-mosaic",
    "volto-ckeditor",
    "volto-plotlycharts",
    "volto-base",
    "volto-embed",
    "volto-tabsview"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "api"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js(x)?$": "babel-jest",
      "^.+\\.css$": "jest-css-modules",
      "^.+\\.scss$": "jest-css-modules",
      "^.+\\.(png)$": "jest-file",
      "^.+\\.(jpg)$": "jest-file",
      "^.+\\.(svg)$": "./node_modules/@plone/volto/jest-svgsystem-transform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@plone/volto).+\\.js$"
    ],
    "moduleNameMapper": {
      "@plone/volto/babel": "<rootDir>/node_modules/@plone/volto/babel",
      "@plone/volto/(.*)$": "<rootDir>/node_modules/@plone/volto/src/$1",
      "@package/(.*)$": "<rootDir>/src/$1",
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "setupFiles": [
      "@plone/volto/test-setup.js"
    ],
    "globals": {
      "__DEV__": true
    }
  },
  "customizationPaths": [
    "src/customizations/volto/"
  ],
  "addonsCustomizationPath": "src/customizations",
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.overrides",
        "options": {
          "parser": "less"
        }
      }
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-idiomatic-order",
      "./node_modules/prettier-stylelint/config.js"
    ],
    "plugins": [
      "stylelint-prettier"
    ],
    "rules": {
      "prettier/prettier": true,
      "rule-empty-line-before": [
        "always-multi-line",
        {
          "except": [
            "first-nested"
          ],
          "ignore": [
            "after-comment"
          ]
        }
      ]
    },
    "ignoreFiles": "theme/themes/default/**/*.overrides"
  },
  "engines": {
    "node": "^10 || ^12"
  },
  "bundlesize": [
    {
      "path": "build/public/static/js/*.js",
      "maxSize": "680kB"
    }
  ],
  "bundlewatch": {
    "files": [
      {
        "path": "build/public/static/js/*.js",
        "maxSize": "680kB"
      }
    ]
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "resolutions": {
    "browserslist": "4.8.3"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.30.0",
    "@blueprintjs/icons": "^3.19.0",
    "@datapunt/matomo-tracker-react": "0.0.16",
    "@plone/volto": "github:eea/volto#8.0.0-beta.10",
    "immutable": "^3.8.2",
    "less-loader": "5.0.0",
    "moment-timezone": "^0.5.31",
    "npm": "^6.14.6",
    "react": "^16.13.0",
    "react-component-queries": "^2.3.0",
    "react-highlight-words": "^0.16.0",
    "react-image-gallery": "^0.9.1",
    "react-infinite-scroll-component": "^5.0.4",
    "react-lazy-load-image-component": "^1.5.0",
    "react-loadable": "^5.5.0",
    "react-sizeme": "^2.6.12",
    "react-slick": "^0.24.0",
    "react-stickynode": "^2.1.1",
    "react-toastify": "^5.3.2",
    "react-visibility-sensor": "^5.1.1",
    "semantic-ui-css": "^2.4.1",
    "slick-carousel": "^1.8.1",
    "start": "^5.1.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-bind": "^7.10.5",
    "@babel/plugin-proposal-function-sent": "^7.10.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.5",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "compression-webpack-plugin": "^3.1.0",
    "cypress": "^3.6",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "mr-developer": "^1.4.0",
    "mrs-developer": "1.1.3",
    "postcss-overrides": "3.1.4",
    "prettier": "1.17.0",
    "prettier-eslint": "^9.0.0",
    "prettier-stylelint": "0.4.2",
    "shelljs": "^0.8.4"
  },
  "optimizeLiftPackages": [
    "immutable",
    "react"
  ],
  "manuallyTranspile": [
    "prepend-http",
    "normalize-url",
    "cidr-regex",
    "url-regex"
  ]
}